{
  "name": "clean-node-debt-management",
  "version": "2.5.1",
  "description": "NodeJs Rest API using TDD, Clean Architecture and Typescript",
  "author": "Nelson Lima",
  "license": "",
  "repository": "",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "check": "npm-check -s -u",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --runInBand --coverage",
    "test:unit": "npm test -- --watchAll -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.5",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.12",
    "@types/jest": "^25.2.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.17",
    "@types/node": "^14.0.1",
    "@types/supertest": "^2.0.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.0.0",
    "eslint-config-standard-with-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^4.1.0",
    "git-commit-msg-linter": "^2.4.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "mockdate": "^2.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.5.7",
    "nodemon": "^2.0.4",
    "slugify": "^1.4.0",
    "swagger-ui-express": "^4.1.4",
    "validator": "^13.0.0"
  },
  "engines": {
    "node": "12.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
